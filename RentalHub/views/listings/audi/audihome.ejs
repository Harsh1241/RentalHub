<% layout("/layouts/boilerplate") %>
    <h3>All available Auditoriums</h3>
    <form method="get" action="/listings/Audi/new">
        <button class="main-add">Add Auditoriums & Get paid</button>
    </form>
    <div class="row row-cols-lg-3 row-clos-md-2 row-cols-sm-1">
        <% for (let listing of allListings){ %>
            <a href="/listings/Audi/<%= listing._id %>" class="listing-link mt-2">
            <div class="card col listing-card">
                <img src="<%= listing.image.url %>" class="card-img-top" alt="listing_image"
                    style="height: 20rem;">
                <div class="card-img-overlay"></div>
                <div class="card-body">
                    <p class="card-text">
                        <b>
                            <%= listing.title %>
                        </b> <br>
                        &#8377; <%= listing.price.toLocaleString("en-IN") %> /Day
                    </p>
                </div>
            </div>
        </a>
        <% } %>
    </div>